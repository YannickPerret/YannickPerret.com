  {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true,
      "mocha": true,
      "node": true,
      "es6": true
    },
    "rules": {
      // Other rules
      "no-console": "off",
      "valid-jsdoc": ["error", {"requireReturn": false}],
      "eqeqeq": "error"
    }
  }
  

